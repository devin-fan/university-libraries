<!-- Navigation -->
<%= render partial: "partials/nav" %>
<div class="container" id="session_container">
  <h3>Log In</h3>

  <%= form_tag sessions_path do %>

  	<div class="control-group">
      <%= label_tag :andrewid %>
      <div class="controls">
        <%= text_field_tag :andrewid, params[:andrewid] %>
      </div>
    </div>

  	<div class="control-group">
      <%= label_tag :password %>
      <div class="controls">
        <%= password_field_tag :password %>
      </div>
    </div>

    <div class="actions"><%= submit_tag "Log In", :class=> "btn indigo darken-4"%></div>
  <% end %>
</div>

<%= render partial: "partials/footer" %>